{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "settings": {
    "react": {
      "version": "17.0.1"
    }
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
